        <script>

            $(document).ready(function(){

                // Masques
                //$('input[type="tel"]').inputmask("99 99 99 99 99");
                $('#registration_directNumber, #admin_account_directNumber').inputmask("999");  //static mask

                $('#antenne_postcode, #organization_postcode').inputmask("99999");  //static mask

                $('#antenne_city, #registration_lastName, #admin_account_lastName, #organization_city, #interlocutor_lastName').keyup(function(){
                    $(this).val($(this).val().toUpperCase());
                });

                $(function () {
                    $('[data-toggle="tooltip"]').tooltip({html : true, trigger: 'focus'})
                });

                $(function () {
                    $('[data-toggle="user-tooltip"]').tooltip({html : true, trigger: 'hover'})
                });
                                

            });


        </script>


        
